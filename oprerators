#include <iostream>
using namespace std;

int main ()
{
      int A,B;
      cout<<"Please Enter First Number A :"<<"\n";
      cin>>A;
      
      cout<<"Please Enter Seconde Number B :"<<"\n";
      cin>>B;
      
      cout<<"First Number + Seconde Number = "<<A + B<<endl;
      cout<<"First Number - Seconde Number = "<<A - B<<endl;
      cout<<"First Number * Seconde Number = "<<A * B<<endl;
      cout<<"First Number / Seconde Number = "<<A / B<<endl;
      cout<<"First Number \% Seconde Number = "<<A % B<<endl;
}


#include <iostream>
using namespace std;

int main ()
{
      
      int A,B;
      cout<<"please enter A?"<<endl;
      cin>>A;
      
      cout<<"please enter B?"<<endl;
      cin>>B;
      cout<<A<<" = "<<B<<" is "<< (A==B) <<endl;
      cout<<A<<" != "<<B<<" is "<< (A!=B) <<endl;
      cout<<A<<" > "<<B<<" is "<< (A>B) <<endl;
      cout<<A<<" < "<<B<<" is "<< (A<B) <<endl;
      cout<<A<<" >= "<<B<<" is "<< (A>=B) <<endl;
      cout<<A<<" <= "<<B<<" is "<< (A<=B) <<endl;
      
      return 0;
}

#include <iostream>
using namespace std;

int main ()
{
      int A,B;
      cout<<"Please Enter First Number A :"<<"\n";
      cin>>A;
      
      cout<<"Please Enter Seconde Number B :"<<"\n";
      cin>>B;
      
      cout<<"First Number + Seconde Number = "<<A + B<<endl;
      cout<<"First Number - Seconde Number = "<<A - B<<endl;
      cout<<"First Number * Seconde Number = "<<A * B<<endl;
      cout<<"First Number / Seconde Number = "<<A / B<<endl;
      cout<<"First Number \% Seconde Number = "<<A % B<<endl;
}

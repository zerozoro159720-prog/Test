#include <iostream>
#include <string>
using namespace std;


int FirstNumber, SecondNumber; // ← متغيرات عامة

int CalculateArea()
{

    int area = FirstNumber * SecondNumber;
    return area;
}

int main()
{
    cout<<"Enter First Number :"<<endl;
    cin>>FirstNumber;
    
    cout<<"Enter Sec Number :"<<endl;
    cin>>SecondNumber;
    
    cout << "Area is: " << CalculateArea() << endl;
    return 0;
}

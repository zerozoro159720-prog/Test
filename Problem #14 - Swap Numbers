#include <iostream>
#include <string>
using namespace std;

void procedure()
{
	int FirstNumber, SecondeNumber, tepm;

	cout << "Enter First Number : " << endl;
	cin >> FirstNumber;

	cout << "Enter Seconde Number : " << endl;
	cin >> SecondeNumber;

	cout << "****************************" << endl;

	cout << FirstNumber << endl << SecondeNumber << endl;

	tepm = FirstNumber;
	FirstNumber = SecondeNumber;
	SecondeNumber = tepm;
	
	cout << FirstNumber << endl << SecondeNumber << endl;
}

int main ()
{
	procedure();
	return 0;
}
